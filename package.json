{
  "name": "ng-samurai",
  "version": "0.0.0-development",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "format": "prettier src/**/*.{ts,json,md} --write",
    "format:test": "prettier src/**/*.{ts,json,md} --list-different",
    "test": "mocha src/**/*.spec.ts --require ts-node/register --timeout 5000",
    "test:watch": "mocha src/**/*.spec.ts --require ts-node/register --watch-extensions ts --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "schematics"
  ],
  "author": "Kevin Kreuzer",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.0.0",
    "@angular-devkit/schematics": "^9.0.0",
    "@schematics/angular": "^9.0.0",
    "boxen": "^4.2.0",
    "chalk": "^4.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.10.59",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "mocha": "^7.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.6.2",
    "semantic-release": "^17.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kreuzerk/ng-samurai.git"
  }
}
